    "gulp-eslint": "1.0.0",
    "gulp-flatten": "0.0.4",
    "gulp-mocha": "2.1.3",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.3",
    "gulp-scss-lint": "0.3.6",
    "gulp-util": "3.0.6",
    "gulp-watch": "4.3.5",
    "karma": "0.13.9",
    "karma-angular": "0.0.6",
    "karma-chai": "0.1.0",
    "karma-coverage": "0.2.7",
    "karma-jspm": "2.0.1",
    "karma-junit-reporter": "0.3.4",
    "karma-mocha": "0.2.0",
